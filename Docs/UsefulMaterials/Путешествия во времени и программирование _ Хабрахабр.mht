    }

    code .variable,
    code .envvar,
    code .apache .sqbracket,
    code .nginx .built_in {
      color: #968e5b;
    }

    code .addition {
      background: #baeeba;
    }

    code .deletion {
      background: #ffc8bd;
    }

    code .diff .change {
      background: #bccff9;
    }

