  -moz-opacity: 0.3;
  filter: alpha(opacity=3D30);
  opacity: 0.3;
  display: none;=20
}
.to_top .to_top_panel {
  width:100%;
  height: 100%;
  font-size: 11px;
  color: #4d7285;
  padding-top: 10px;=20
  position: relative;
}
.to_top:hover {
  -moz-opacity: 0.9;
  filter: alpha(opacity=3D90);
  opacity: 0.9;=20
}
.to_top:hover .to_top_panel {
  cursor: pointer;
  background: #f0f0f0;=20
}
.to_top .close_panel {
	position: absolute;
	bottom: 20px;
	left: 0px;
	right: 0px;
	text-align: center;
  line-height: 8px;
  font-family: tahoma, arial, verdana, sans-serif;=20
  display: none;
}
.to_top:hover .close_panel{
